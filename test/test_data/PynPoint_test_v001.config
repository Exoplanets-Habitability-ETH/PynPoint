[workspace]
workdir = NotSetfortest
datadir = Notsetfortest


[module1]
mod_type = images 	; type of module to run: images, basis or residuals
input = test_data/	; inputfile name
intype =	dir			; input type
options = options1

[module2]
mod_type = basis	; type of module to run: images, basis or residuals
input =	test_data/		; inputfile name
intype = dir			; input type
options = options1

[module3]
mod_type = residuals
intype = instances
images_input = module1
basis_input = module2


[options1]
cent_remove=True	; mask image center (and edge): True or False 
cent_size=0.2		; size of the cental mask: 0.0 -> 1.0
edge_size=1.0		; size of the outer mask: 0.0 ->1
resize=True			; whether to resize the image: True or False
F_final=2			; the factor by which the size will be changed:number 	
recent=False 		; whether to recenter the images: True or False
F_int=4				; the resize factor used for the recentering
ran_sub=False		; whether to use a subset of the images: False or number	
para_sort=True		; whether to sort by para angle: True or False
inner_pix=False		; whether to mask the inner pixels: False or number of pix

